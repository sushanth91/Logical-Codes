import java.lang.*;
import java.io.*;
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        String str = "Hello";
        StringBuilder newStr = new StringBuilder();
        newStr.append(str);
        newStr.reverse();
        System.out.println(newStr);
    }
}

class HelloWorld {
    public static void main(String[] args) {
        String str = "Hello";
    for(int i=str.length()-1; i>=0; --i){
        System.out.print(str.charAt(i));
    }
    }
}
